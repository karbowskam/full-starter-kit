{
  "name": "my-starter-kit",
  "version": "1.0.0",
  "description": "My starter kit",
  "main": "index.js",
  "scripts": {
    "start": "npm run test && npm run build-dev",
    "build": "npm run test && npm build-prod",
    "build-dev": "webpack-dev-server --config webpack.dev.js --mode development",
    "build-prod": "webpack --config webpack.prod.js --mode production",
    "publish": "npm run build && npm run publish-cmd",
    "publish-cmd": "gh-pages -d dist",
    "test": "npm run test-html && npm run test-html-lint && npm run test-css && npm run test-css-lint && npm run test-js-lint && npm run test-js",
    "test-html": "echo HTML Testing && gulp validate-html",
    "test-html-lint": "echo HTML Linting && gulp validate-html-lint",
    "test-css": "echo CSS Testing && gulp validate-css",
    "test-css-lint": "echo CSS Linting && npx stylelint \"src/scss/*.scss\"",
    "test-js-lint": "echo JS Linting && gulp validate-js-lint",
    "test-js": "echo JS Testing"
  },
  "author": "Marta Karbowska",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "autoprefixer": "^9.8.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.1",
    "core-js": "^3.6.5",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "gh-pages": "^2.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-eslint": "^6.0.0",
    "gulp-htmllint": "0.0.19",
    "gulp-sass": "^4.1.0",
    "gulp-w3c-html-validator": "^2.0.1",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "map-stream": "0.0.7",
    "mini-css-extract-plugin": "^0.9.0",
    "minimist": "^1.2.3",
    "node-sass": "^4.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.7.0",
    "stylelint-config-standard": "^20.0.0",
    "terser-webpack-plugin": "^3.0.2",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "4.2.2",
    "websocket-extensions": "^0.1.4"
  },
  "dependencies": {
    "gulp-w3c-css": "^2.0.0",
    "normalize.css": "^8.0.1"
  }
}
